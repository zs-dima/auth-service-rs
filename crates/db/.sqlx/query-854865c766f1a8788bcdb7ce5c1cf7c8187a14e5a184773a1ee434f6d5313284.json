  "db_name": "PostgreSQL",
  "query": "\n            SELECT u.id,\n                   u.role,\n                   u.email,\n                   u.email_verified,\n                   u.phone,\n                   u.phone_verified,\n                   u.status AS \"status: UserStatus\",\n                   u.password,\n                   u.failed_login_attempts,\n                   u.locked_until,\n                   u.created_at,\n                   u.updated_at,\n                   u.deleted_at,\n                   p.display_name,\n                   p.avatar_url,\n                   p.locale,\n                   p.timezone\n              FROM auth.users u\n              JOIN auth.user_profiles p ON p.id_user = u.id\n             WHERE u.email = $1\n             LIMIT 1\n            ",
